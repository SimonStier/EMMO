_:genid8 a owl:Axiom ;
	owl:annotatedSource :EMMO_43e9a05d_98af_41b4_92f6_00f79a09bfce ;
	owl:annotatedProperty :b81b8844_fa6f_4a4c_a6a7_dda97b1bf853 ;
