   owl:annotatedProperty skos:prefLabel ;
   owl:annotatedTarget "EMMO"@en ;
   :EMMO_705f27ae_954c_4f13_98aa_18473fc52b25 "EMMO is the acronym of Elementary Multiperspective Material Ontology."@en
 ] .


